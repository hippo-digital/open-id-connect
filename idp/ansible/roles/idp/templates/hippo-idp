server {
    listen 443;
    server_name login.hippo.digital;

    ssl on;
    ssl_certificate /home/user1/login.hippo.digital.pem;
    ssl_certificate_key /home/user1/login.hippo.digital.key;

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/home/user1/open-id-connect/idp/web/hippo-idp.sock;
    }
}